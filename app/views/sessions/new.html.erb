<%= form_with(model: @user, url: sessions_path) do |form| %>

<div class="form-group">
  <%= form.label :email %>
  <%= form.text_field :email, id: :user_email, class: 'form-control' %> <br>
</div>

<div class="form-group">
  <%= form.label :password %>
  <%= form.password_field :password, id: :user_password, class: 'form-control'%>
</div>


<%= form.submit "Ingresar", class: 'btn btn-primary' %>
<% end %>